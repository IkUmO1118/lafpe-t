{
  "name": "ikumo1118/backend",
  "scripts": {
    "server": "php -S localhost:8000",
    "test": "./vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Controllers\\": "app/Controllers/",
      "Services\\": "app/Services/",
      "Contracts\\": "app/Services/Contracts/",
      "Config\\": "config/",
      "Helpers\\": "Helpers/",
      "Response\\": "Response/",
      "Render\\": "Response/Render/",
      "Routing\\": "Routing/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Feature\\": "tests/Feature/",
      "Tests\\Integration\\": "tests/Integration/",
      "Tests\\Unit\\": "tests/Unit/",
      "Tests\\Unit\\Controllers\\": "tests/Unit/Controllers/",
      "Tests\\Unit\\Services\\": "tests/Unit/Services/",
      "Tests\\Unit\\Helpers\\": "tests/Unit/Helpers/"
    }
  },
  "authors": [
    {
      "name": "Ikumo Takahashi",
      "email": "ikumo1118free@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10",
    "mockery/mockery": "^1.6"
  }
}
